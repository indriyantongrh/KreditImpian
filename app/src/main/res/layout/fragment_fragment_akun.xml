        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linera23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:padding="0dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/cosimabold"
                android:paddingStart="13dp"
                android:text="Akun Saya"
                android:textColor="@color/colorwhite"
                android:textSize="18sp" />

            <androidx.cardview.widget.CardView
                android:id="@+id/card1"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="5dp"
                android:background="#00ffffff"
                android:elevation="@dimen/padding_10"
                app:cardCornerRadius="12dp"
                app:cardElevation="5dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/image"
                        android:layout_width="90dp"
                        android:layout_height="80dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:clickable="true"
                        android:elevation="5dp"
                        android:focusable="true"
                        android:foregroundGravity="center"
                        android:scaleType="centerCrop"
                        android:src="@drawable/icon_user"
                        android:visibility="gone"
                        app:civ_border_color="#FFFFFFFF"
                        app:civ_border_width="3dp"
                        app:civ_fill_color="#efeeee" />


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/image"
                        android:gravity="center">

                        <TextView
                            android:id="@+id/txthai"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/image"
                            android:fontFamily="@font/cosimabold"
                            android:gravity="center"
                            android:text="Selamat datang kembali "
                            android:textAppearance="@android:style/TextAppearance.WindowTitle"
                            android:textColor="@color/colorOrange"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/txt_nama_akun"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:layout_toRightOf="@+id/txthai"
                            android:fontFamily="@font/cosimabold"
                            android:gravity="center"
                            android:text="Nama"
                            android:textAppearance="@android:style/TextAppearance.WindowTitle"
                            android:textColor="@color/colorOrange"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>

